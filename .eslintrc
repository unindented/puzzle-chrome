{
  "extends": ["standard", "standard-react"],
  "parser": "babel-eslint",
  "rules": {
    "no-unused-vars": [2, {
      "args": "after-used",
      "argsIgnorePattern": "^_",
      "varsIgnorePattern": "^_"
    }]
  },
  "env": {
    "browser": true,
    "worker": true,
    "jasmine": true
  },
  "globals": {
    "chrome": true
  }
}
